<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <welcome-file-list>
        <welcome-file>/login.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteUser</servlet-name>
        <servlet-class>servlet.deleteUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addUser</servlet-name>
        <servlet-class>servlet.addUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getDonation</servlet-name>
        <servlet-class>servlet.getDonation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getRec</servlet-name>
        <servlet-class>servlet.getRec</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addDon</servlet-name>
        <servlet-class>servlet.addDon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addRec</servlet-name>
        <servlet-class>servlet.addRec</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateUser</servlet-name>
        <servlet-class>servlet.updateUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getProv</servlet-name>
        <servlet-class>servlet.getProv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deLog</servlet-name>
        <servlet-class>servlet.deLog</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteUser</servlet-name>
        <url-pattern>/deleteUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addUser</servlet-name>
        <url-pattern>/addUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getDonation</servlet-name>
        <url-pattern>/getDonation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getRec</servlet-name>
        <url-pattern>/getRec</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addDon</servlet-name>
        <url-pattern>/addDon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addRec</servlet-name>
        <url-pattern>/addRec</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateUser</servlet-name>
        <url-pattern>/updateUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getProv</servlet-name>
        <url-pattern>/getProv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deLog</servlet-name>
        <url-pattern>/deLog</url-pattern>
    </servlet-mapping>


</web-app>